{
  "name": "my-turborepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "frontend",
    "services/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "dotenv-cli": "7.0.0",
    "esbuild": "0.17.5",
    "esbuild-plugin-glob": "2.2.1"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.17.12",
    "@babel/eslint-parser": "7.17.0",
    "@babel/node": "7.20.7",
    "@babel/preset-env": "7.17.12",
    "cross-env": "5.1.4",
    "eslint": "8.33.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-turbo": "0.0.7",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "2.6.0",
    "husky": "8.0.3",
    "prettier": "2.8.3",
    "rimraf": "4.1.2",
    "shx": "0.3.4",
    "turbo": "1.7.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.5.0"
}